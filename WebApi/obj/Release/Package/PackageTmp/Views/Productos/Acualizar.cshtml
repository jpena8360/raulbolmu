<head>
    <!-- CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">

    <script src="https://code.jquery.com/jquery-3.0.0.js" crossorigin="anonymous"></script>
    <!-- jQuery and JS bundle w/ Popper.js -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ho+j7jyWK8fNQe+A12Hb8AhRq26LrZ/JpcUGGOn+Y7RsweNrtN/tE3MoK7ZeZDyx" crossorigin="anonymous"></script>

</head>

@model List<model.entity.Productos>
@{ ViewBag.Title = "Acualizar"; }
<br />
<br />
<br />
<div class="container-fluid">

    <form>
        <div>
            <button type="button" class="btn btn-primary" onclick="ActualizarEstudiantes()">Actualizar</button>
        </div><br>
        <table class="table">
            <tr class="btn-primary">
                <th scope="col">Codigo_Transporte</th>
                <th scope="col">Nombre</th>
                <th scope="col">Direccion</th>
                <th scope="col">Telefono</th>
                <th scope="col">Tipo_Producto</th>
                <th scope="col">Cantidad_Producto</th>
                <th scope="col">Fecha_registro</th>
                <th scope="col">Medio_Entrega</th>
                <th scope="col">Bodega_Entrega</th>
                <th scope="col">Puerto_Entrega</th>
                <th scope="col">Precio_envio</th>
                <th scope="col">Medio_Transporte</th>
                <th scope="col">Placa_vehículo</th>
                <th scope="col">Numero_guía</th>
                <th scope="col">Numero_Flota</th>
                @*<th scope="col">Total</th>*@
            </tr>
            @foreach (var objProducto in Model)
            {
<tr>
    <td> <input type="text" id="Id_Envio" value="@objProducto.Id_Envio" /></td>
    <td> <input type="text" id="Nombre" value="@objProducto.Nombre" /></td>
    <td> <input type="text" id="Direccion" value="@objProducto.Direccion" /></td>
    <td> <input type="text" id="Telefono" value="@objProducto.Telefono" /></td>
    <td> <input type="text" id="Tipo_producto" value="@objProducto.Tipo_producto" /></td>
    <td> <input type="text" id="Cantidad_Producto" value="@objProducto.Cantidad_Producto" /></td>
    <td> <input type="text" id="Fecha_registro" value="@objProducto.Fecha_registro" /></td>
    <td> <input type="text" id="Fecha_entrega" value="@objProducto.Fecha_entrega" /></td>
    <td> <input type="text" id="Medio_Entrega" value="@objProducto.Medio_Entrega" /></td>
    <td> <input type="text" id="Bodega_Entrega" value="@objProducto.Bodega_Entrega" /></td>
    <td> <input type="text" id="Puerto_Entrega" value="@objProducto.Puerto_Entrega" /></td>
    <td> <input type="text" id="Precio_envio" value="@objProducto.Precio_envio" /></td>
    <td> <input type="text" id="Medio_Transporte" value="@objProducto.Medio_Transporte" /></td>
    <td> <input type="text" id="Placa_vehículo" value="@objProducto.Placa_vehículo" /></td>
    <td> <input type="text" id="Numero_Flota" value="@objProducto.Numero_Flota" /></td>
    <td> <input type="text" id="Numero_guía" value="@objProducto.Numero_guía" /></td>
</tr>
}

        </table>
    </form>
</div>

<script src="~/Scripts/Editar.js"></script>



