</head>

@model List<model.entity.Registros>
@{ ViewBag.Title = "Acualizar"; }
<br />
<br />
<br />
<div class="container-fluid">

    <form>
        <div>
            <button type="button" class="btn btn-primary" onclick="ActualizarEstudiantes()">Actualizar</button>
        </div><br>
        <table class="table">
            <tr class="btn-primary">
                <th scope="col">Pais</th>
                <th scope="col">Nombre</th>
                @*<th scope="col">PaisID</th>*@
                @*<th scope="col">UsuarioID</th>*@
                <th scope="col">Tipo</th>
                <th scope="col">Arranque</th>
                <th scope="col">Envion</th>
                <th scope="col">TotalPeso</th>
                <th scope="col">Intentos</th>
            </tr>
            @foreach (var objProducto in Model)
            {
        <tr>
            <td> <input type="text" id="PaisNombre" value="@objProducto.PaisNombre" /></td>
            <td> <input type="text" id="Nombre" value="@objProducto.Nombre" /></td>
            <td> <input type="text" id="Tipo" value="@objProducto.Tipo" /></td>
            <td> <input type="text" id="Arranque" value="@objProducto.Arranque" /></td>
            <td> <input type="text" id="Envion" value="@objProducto.Envion" /></td>
            <td> <input type="text" id="TotalPeso" value="@objProducto.TotalPeso" /></td>
            <td> <input type="text" id="NumIntentos" value="@objProducto.NumIntentos" /></td>

            <td> <input type="hidden" id="PaisID" value="@objProducto.PaisID" /></td>
            <td> <input type="hidden" id="UsuarioID" value="@objProducto.UsuarioID" /></td>
            <td> <input type="hidden" id="RegistroID" value="@objProducto.RegistroID" /></td>
            <td> <input type="hidden" id="NumIntentos" value="@objProducto.NumIntentos  " /></td>
        </tr>}

        </table>
    </form>
</div>

<script src="~/Scripts/Editar.js"></script>




